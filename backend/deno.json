{
  "tasks": {
    "dev": "deno run --allow-net --allow-env --allow-read --watch src/main.ts",
    "start": "deno run --allow-net --allow-env --allow-read src/main.ts",
    "migrate": "deno run --allow-net --allow-env --allow-read src/db/migrate.ts",
    "init-admin": "deno run --allow-net --allow-env --allow-read src/db/init-admin.ts",
    "init": "deno task migrate && deno task init-admin"
  },
  "imports": {
    "@oak/oak": "jsr:@oak/oak@17.1.2",
    "bcrypt": "https://deno.land/x/bcrypt@v0.4.1/mod.ts",
    "djwt": "https://deno.land/x/djwt@v3.0.1/mod.ts",
    "drizzle-orm": "npm:drizzle-orm@^0.33.0",
    "mysql2": "npm:mysql2@^3.9.0",
    "nanoid": "npm:nanoid@^5.0.0"
  }
}
